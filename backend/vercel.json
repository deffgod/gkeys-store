{
  "version": 2,
  "buildCommand": "npm run build:deploy",
  "framework": null,
  "installCommand": "npm install",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "dist/**",
      "maxDuration": 60
    }
  }
}
